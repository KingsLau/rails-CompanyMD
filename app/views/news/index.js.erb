var content = `<%= render 'index', news_array: @news_array %>`
var displayNewsOnClick = (content) => {
  const newsDiv = document.querySelector("#news-container")
  newsDiv.innerHTML = content
};

if (document.querySelector('#news-container').dataset.loaded == "false") {
  displayNewsOnClick(content);
  document.querySelector('#news-container').dataset.loaded = "true";
} else {
  console.log('test');
}
